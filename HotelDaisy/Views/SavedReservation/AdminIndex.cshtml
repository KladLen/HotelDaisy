@using HotelDaisy.Models.ViewModels;
@model List<ReservationWithUserFullName>
@{
    ViewData["Title"] = "Reservation Manager";
}

<h1 class="display-5 pb-4">Upcoming reservations</h1>

<div style="height: 250px; overflow-y: auto">
    @await Html.PartialAsync("_ReservationTablePartial", Model)
</div>
<a asp-controller="SavedReservation" asp-action="Search" class="btn btn-secondary mt-3"><i class="bi bi-search me-2"></i>Search reservation</a>
